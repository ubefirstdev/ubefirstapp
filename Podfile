pod 'FBSDKShareKit'
pod 'Firebase/Core'
pod 'Firebase/Database'
pod 'Firebase/Storage'
pod 'GoogleSignIn'
pod 'Firebase/Firestore'
pod 'IQKeyboardManager'
pod 'SideMenu'


end
